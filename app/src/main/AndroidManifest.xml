<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cheenu123.chaitanyasPriceList">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design">
        <activity
            android:name="com.cheenu123.chaitanyasPriceList.AddItemActivity"
            android:exported="false"
            android:label="Add New Item"
            android:parentActivityName="com.cheenu123.chaitanyasPriceList.PriceListActivity"/>
        <activity
            android:name="com.cheenu123.chaitanyasPriceList.PriceListActivity"
            android:exported="false"
            android:label="Price List"
            android:parentActivityName="com.cheenu123.chaitanyasPriceList.MainActivity" />
        <activity
            android:name="com.cheenu123.chaitanyasPriceList.MainActivity"
            android:exported="true"
            android:label="Chaitanya's Store">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>